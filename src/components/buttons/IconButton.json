{
  "component": "IconButton",
  "sizes": ["mini", "sm", "md", "lg"],
  "layout": {
    "type": "auto-layout",
    "direction": "horizontal",
    "alignment": {
      "self": "stretch",
      "children": "center"
    },
    "padding": {
      "top": 8,
      "right": 8,
      "bottom": 8,
      "left": 8
    },
    "gap": 0
  },
  "constraints": {
    "horizontal": "center",
    "vertical": "center",
    "width": {
      "mode": "fixed",
      "min": null,
      "max": null,
      "value": 40
    },
    "height": {
      "mode": "fixed",
      "min": null,
      "max": null,
      "value": 40
    }
  },
  "overflow": {
    "horizontal": "visible",
    "vertical": "visible",
    "clipping": false
  },
  "text": {
    "truncation": "none",
    "whiteSpace": "normal",
    "maxLines": null,
    "overflowWrap": "normal"
  },
  "children": [
    {
      "element": "icon",
      "constraints": {
        "horizontal": "center",
        "vertical": "center"
      },
      "flex": {
        "grow": 0,
        "shrink": 0,
        "basis": "auto"
      },
      "width": {
        "mode": "fixed",
        "value": 16
      },
      "height": {
        "mode": "fixed",
        "value": 16
      }
