    "md": {
      "width": {
        "value": 40
      },
      "height": {
        "value": 40
      },
      "padding": {
        "top": 8,
        "right": 8,
        "bottom": 8,
        "left": 8
      },
      "iconSize": 16
    },
    "lg": {
      "width": {
        "value": 48
      },
      "height": {
        "value": 48
